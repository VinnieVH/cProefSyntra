<% include ../partials/header %>
<main>
    <div class="container">
        <h1 class="clear-fix"> Summer. Undiscovered.</h1>
        <div class="jumbotron"></div>
        <div class="row">
            <% products.forEach(function(product){ %>
            <div class="col-xs-6 col-md-3">
                <a href="https://www.google.com">
                    <div class="thumbnail">
                        <img class="product-image" src="<%=product.image%>">
                    <div class="item-content">
                        <h4 class="item-brand"><%= product.brand %></h4>
                        <h4 class="item-price"><%= product.price %></h4>
                        <h4 class="item-category"><%= product.category %></h4>
                    </div>
                </a>
                <div class="item-options">
                    <a href="https://www.google.com" type="button" class="item-shoppingbag btn btn-default"><i class="fas fa-shopping-bag"></i></a>
                    <a href="https://www.facebook.com" type="button" class="item-wishlist btn btn-default"><i class="fas fa-heart"></i></a>
                </div>
            </div>
        </div>
    <% }); %>
      </div>
    </div>
</main>

<% include ../partials/footer %>